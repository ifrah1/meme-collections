<!-- partial for head -->
<%- include('../partials/header') %>
    <%- include('../partials/nav') %>
        <div class="d-flex justify-content-center mb-3">
            <div class="category-header col-3 shadow p-3 mb-3 bg-white rounded text-center mx-1 text-primary display-6">
                <%= category.name %>
            </div>
        </div>
        <div class="d-flex justify-content-center mb-3">
            <form action="/memes/<%= category._id %>/addmeme" method="POST">
                <label>
                    <input type="text" name="name" placeholder="Paste here"
                        class="shadow-none p-2 mb-4 bg-light rounded">
                </label>
                <input type="submit" value="Add URL" class="btn btn-danger text-white">

            </form>
        </div>
        <!-- partial for footer -->
        <%- include('../partials/footer') %>