        <div class="d-flex justify-content-center mb-3">
            <div class="category-header col-3 shadow p-3 mb-3 bg-white rounded text-center mx-1 text-primary display-6">
                <%= category.name %>
            </div>
        </div>
        <div class="container d-flex justify-content-center mb-3">
            <form action="/memes/<%= category._id %>/addmeme" method="POST" enctype="multipart/form-data">

